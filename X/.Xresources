Xft.dpi: 168
Xft.autohint: 0
Xft.lcdfilter:  lcddefault
Xft.hintstyle:  hintfull
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb
*faceName: inconsolata, font-awesome

! disable ctrl-shift
URxvt*iso14755_52: false
URxvt*iso14755: false

URxvt*scrollBar: false
URxvt*scrollBar_right: false
URxvt*background: #1d1f21
URxvt*foreground: #c5c8c6
URxvt*font: xft:UbuntuMono Nerd Font Mono:autohint=true
URxvt*italicFont: xft:UbuntuMono Nerd Font Mono:autohint=true:italic:minspace=False
URxvt*saveLines: 10000
URxvt*scrollTtyOutput:      false
URxvt*scrollWithBuffer:     true
URxvt*scrollTtyKeypress:    true

URxvt.perl-ext-common: default,selection-to-clipboard,pasta,matcher,keyboard-select
URxvt.keysym.M-u: perl:url-select:select_next
URxvt.url-launcher: /usr/bin/firefox
URxvt.underlineURLs: True
URxvt.matcher.button: 1 
URxvt.keysym.M-Escape: perl:keyboard-select:activate
URxvt.keysym.Control-Shift-V: perl:pasta:paste
URxvt.letterSpace: -2

! Color theme: terminal.sexy -> collection/hybrid
! special
*.foreground:   #c5c8c6
*.background:   #1d1f21
*.cursorColor:  #c5c8c6

! black
